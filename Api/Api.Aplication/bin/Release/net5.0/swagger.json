  "definitions": {
    "LoginDto": {
      "required": [
        "email"
      ],
      "type": "object",
      "properties": {
        "email": {
          "format": "email",
          "maxLength": 100,
          "minLength": 0,
          "type": "string"
        },
        "senha": {
          "maxLength": 20,
          "minLength": 0,
          "type": "string"
        },
        "authToken": {
          "type": "string"
        },
        "idToken": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "photoUrl": {
          "type": "string"
        },
        "provider": {
          "type": "string"
        }
      }
    },
    "UsuarioPostDto": {
      "required": [
        "desEmail",
        "desSenha"
      ],
      "type": "object",
      "properties": {
        "desImagem": {
          "type": "string"
        },
        "desEmail": {
          "format": "email",
          "maxLength": 100,
          "minLength": 0,
          "type": "string"
        },
        "desSenha": {
          "maxLength": 20,
          "minLength": 0,
          "type": "string"
        },
        "desTelefone": {
          "maxLength": 20,
          "minLength": 0,
          "type": "string"
        }
      }
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "Entre com o Token JWT"
    }
  },
  "security": [
    {
